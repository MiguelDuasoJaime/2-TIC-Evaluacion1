# -*- coding: cp1252 -*-
def vocales(letra):
    let=["u","o","i","e","a"]
    if letra in let:
        return(True)
    else:
        return(False)
def deletreo(pal):
    cnt=0
    for x in range(0,len(pal)):
        print(pal[x])
        if vocales(pal[x]) == True:
            cnt+=1
    print("El nยบ de vocales es: " + str(cnt))
deletreo(raw_input("Di una palabra: "))
