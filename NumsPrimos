import math
num=int(input("Introduce un numero: "))
def divisores(n):
    div=0
    for x in range(1,n+1):
        if n%x == 0:
            print(str(x) + " Es divisor de " + str(n))
            div+=1
    if div < 3:
        print(str(n)+ " Es un numero primo")
divisores(num)
